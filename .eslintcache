>{setBooks(bookObject);}, 1000)\r\n            setLoading(false)\r\n        })\r\n\r\n\r\n    }, [])\r\n    \r\n\r\n    return (\r\n        <div>\r\n            <ItemDetail books={books}  add={add} />\r\n        </div>\r\n    )\r\n}\r\n","C:\\Users\\Cecilia\\Desktop\\Front-End\\ReactJs2020\\CoderApp\\coderApp\\coderapp\\src\\containers\\ItemQuantityContainer.js",[],"C:\\Users\\Cecilia\\Desktop\\Front-End\\ReactJs2020\\CoderApp\\coderApp\\coderapp\\src\\componentes\\ItemQuantitySelector\\ItemQuantitySelector.js",[],"C:\\Users\\Cecilia\\Desktop\\Front-End\\ReactJs2020\\CoderApp\\coderApp\\coderapp\\src\\componentes\\AddItemButton\\AddItemButton.js",[],"C:\\Users\\Cecilia\\Desktop\\Front-End\\ReactJs2020\\CoderApp\\coderApp\\coderapp\\src\\containers\\CartContainer.js",[],"C:\\Users\\Cecilia\\Desktop\\Front-End\\ReactJs2020\\CoderApp\\coderApp\\coderapp\\src\\containers\\CategoryContainer.js",["127","128"],"import React, { useEffect, useState }  from 'react';\r\nimport Category from '../componentes/Category/Category';\r\nimport { useCartContext } from \"../context/CartContext\";\r\nimport { getFirestore } from '../firebase/index';\r\nimport { useParams } from 'react-router-dom';\r\n\r\nexport default function CategoryContainer() {\r\n\r\n    const { categoryKey } = useParams();\r\n    const { setLoading } = useCartContext();\r\n    const [books, setBooks] = useState([])\r\n   \r\n    useEffect(() => {\r\n        setLoading(true);\r\n        const db = getFirestore();\r\n        const categoryCollection = db.collection('categories');\r\n        categoryCollection.get().then((query) => {\r\n            if (query.size === 0) {\r\n                console.log('No tiene resultados')\r\n            }\r\n            return query.docs.find(doc => \r\n    \r\n                doc.data().key == categoryKey\r\n            \r\n            ).id\r\n            \r\n        }).then((categoryId) => {\r\n            const itemcollection = db.collection(\"products_books\")\r\n            .